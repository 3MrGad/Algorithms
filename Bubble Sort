#include<iostream>
using namespace std;
void Sorting (int arr[],int Size);//bubble Sort
int main ()
{
    int Size;
    cout<<"enter the Size \n";
    cin>>Size;
    cout<<"enter elements \n";
    int arr[Size];
    for(int i=0;i<Size;i++)
    {
        cin>>arr[i];
    }
    Sorting (arr,Size);
    for(int i=0;i<Size;i++)
    {
       cout<<i[arr];
    }
    return 0;
}
void Sorting(int arr[],int Size)
{
    for(int i=0;i<Size-1;i++)
    {
        for(int j=0;j<Size-i-1;j++)
        {
            if(arr[j]>arr[j+1])
                {
            int temp;
            temp=arr[j];
            arr[j]=arr[j+1];
            arr[j+1]=temp;
                }
        }
    }
}
